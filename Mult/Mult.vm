// Calculates a = x*y
// a is a local variable
// x & y are static variables

// Put your code here.

push static 0
pop temp 0
push constant 0
pop temp 1

label start
    push temp 0
    push constant 0
    eq
    if-goto finish
    push temp 1
    push static 1
    add
    pop temp 1
    push temp 0
    push constant 1
    sub
    pop temp 0
    goto start

label finish
    push temp 1
    pop local 0
    goto END


label END
goto END